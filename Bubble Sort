#include <stdio.h>

int main()
{
    int n,a[100],t,i,j;
    printf("Size: ");
    scanf("%d",&n);
    
    printf("Array elements are: ");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    
    printf("Before sorting: ");
    for(i=0;i<n;i++)
    {
        printf("%d\n",a[i]);
    }
    
    
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]>a[j])
            {
            t=a[i];
            a[i]=a[j];
            a[j]=t;
            }
        }
    }
    
    
    printf("Sorted array elements are: ");
    for(i=0;i<n;i++)
    {
        printf("%d\n",a[i]);
    }
    return 0;
}
